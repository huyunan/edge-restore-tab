{
   "action": {
      "default_title": "恢复标签页",
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+R"
         }
      },
      "restore-tab": {
         "suggested_key": {
            "default": "Alt+2"
         },
         "description": "恢复关闭标签页"
      }
   },
   "description": "恢复标签页",
   "icons": {
      "16": "icon/icon_16.png",
      "48": "icon/icon_48.png",
      "128": "icon/icon_128.png"
   },
   "manifest_version": 3,
   "name": "恢复标签页",
   "permissions": [ "tabs", "scripting", "activeTab", "commands", "contextMenus", "storage", "sessions" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "2.8"
}
